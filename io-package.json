{
    "common": {
        "name":                     "occ",
        "version":                  "0.1.20",
        "title":                    "Node.js Object Control Calendar Adapter",
        "desc":                     "Javascript/Node.js based Object Control Calendar adapter",
        "platform":                 "Javascript/Node.js",
        "mode":                     "daemon",
        "icon":                     "logo.svg",
        "extIcon":                  "http://fullcalendar.io/images/logo.svg",
        "keywords":                 ["scheduler", "calendar", "fullcalendar", "object control", "homematic"],
        "loglevel":                 "info",
        "localLink":		        "http://%ip%:%web_port%/occ/index.html",
        "dependencies":             ["web", "ical"],
        "readme":                   "https://github.com/husky-koglhof/iobroker.occ/blob/master/README.md",
        "onlyWWW":                  false,
        "singleton":                true,
        "adminTab":                {
            "singleton":    true,
            "name":         "OCC",
            "link":         "http://%ip%:%web_port%/occ/index.html"
        }
    },
    "native": {
        "rpcListenIp": "0.0.0.0",
        "rpcListenPort": 1600,
        "force": false,
        "demo": true,
        "ical": false
    },
    "objects": [

    ],
    "instanceObjects": [
        {
            "_id": "updated",
            "type": "state",
            "common": {
                "name": "Some new devices added",
                "type": "boolean",
                "read":  true,
                "write": true

            }
        }
    ]
}
